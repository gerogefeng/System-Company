<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.materialdesignicons.MaterialDesignIconView?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="by.psu.gui.controllers.department_departure.CDeparDeparture">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <AnchorPane prefHeight="400.0" prefWidth="60.0" style="-fx-background-color: #f0f2f5;" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <VBox spacing="9.0" AnchorPane.rightAnchor="7.0" AnchorPane.topAnchor="7.0">
                     <children>
                        <JFXButton fx:id="departureButton" buttonType="RAISED" contentDisplay="GRAPHIC_ONLY" onAction="#actionGetDepartures" prefHeight="45.0" prefWidth="45.0" styleClass="button_auth" stylesheets="@../style/style.css" textFill="WHITE">
                           <graphic>
                              <MaterialDesignIconView fill="WHITE" glyphName="CALENDAR_CLOCK" size="20" />
                           </graphic>
                        </JFXButton>
                        <Separator prefHeight="0.0" prefWidth="0.0" />
                        <JFXButton fx:id="plusDepartureButton" buttonType="RAISED" contentDisplay="GRAPHIC_ONLY" onAction="#actionPlusDeparture" prefHeight="45.0" prefWidth="45.0" styleClass="button_auth" stylesheets="@../style/style.css" textFill="WHITE">
                           <graphic>
                              <MaterialDesignIconView fill="WHITE" glyphName="PLUS" size="20" />
                           </graphic>
                        </JFXButton>
                        <Separator prefHeight="0.0" prefWidth="0.0" />
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
            <StackPane fx:id="stackPane" prefHeight="150.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="60.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
